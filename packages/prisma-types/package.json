{
  "name": "@awardrobe/prisma-types",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf .turbo node_modules dist",
    "with-env": "dotenv -e ../../.env --",
    "db:generate": "pnpm with-env prisma generate",
    "db:push": "pnpm with-env prisma db push --skip-generate",
    "dev": "pnpm with-env prisma studio --browser none",
    "build": "tsup index.ts --format cjs --dts"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0"
  },
  "devDependencies": {
    "@awardrobe/tsconfig": "workspace:^",
    "@types/node": "^20.10.3",
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.6.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
