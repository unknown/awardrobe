{
  "name": "@awardrobe/prisma-types",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf .turbo node_modules dist",
    "with-env": "dotenv -e ../../.env --",
    "db:generate": "npm run with-env -- prisma generate",
    "db:push": "npm run with-env -- prisma db push --skip-generate",
    "dev": "npm run with-env -- prisma studio --browser none",
    "build": "tsup index.ts --format cjs --dts"
  },
  "dependencies": {
    "@prisma/client": "^5.4.1"
  },
  "devDependencies": {
    "@awardrobe/tsconfig": "*",
    "@types/node": "^20.8.3",
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.4.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
