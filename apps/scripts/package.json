{
  "name": "@awardrobe/scripts",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo node_modules dist",
    "with-env": "dotenv -e ../../.env --",
    "db:seed": "npm run with-env -- ts-node -r tsconfig-paths/register --transpile-only src/seed.ts",
    "scraper": "npm run with-env -- ts-node -r tsconfig-paths/register --transpile-only src/scraper.ts"
  },
  "dependencies": {
    "@awardrobe/adapters": "*",
    "@awardrobe/meilisearch-types": "*",
    "@awardrobe/prisma-types": "*",
    "@awardrobe/proxies": "*",
    "@awardrobe/tsconfig": "*",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.1",
    "dotenv-cli": "^7.3.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
