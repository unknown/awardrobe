    "@awardrobe/tsconfig": "workspace:^",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@awardrobe/tsconfig": "workspace:^",
    "@types/node": "^20.10.3",
    "dotenv-cli": "^7.3.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
